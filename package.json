  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-engine": "tsc -p engine",
    "build-interface": "ng build",
    "build": "npm run build-engine && npm run build-interface",
    "serve-engine": "nodemon --config nodemon.json --watch",
    "serve-interface": "ng --proxy-config proxy.conf.json serve",
    "start": "npm run build-interface && npm run build-engine && node dist/engine/main.js"
